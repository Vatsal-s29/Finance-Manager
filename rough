// bulk add expense
const bulkAddExpense = async (expenses) => {
    try {
        const response = await axiosInstance.post(API_PATHS.EXPENSE.BULK_ADD, {
            expenses,
        });

        setOpenAddExpenseModal(false);
        toast.success("Expense added successfully");
        fetchExpenseDetails();

        return response.data;
    } catch (error) {
        throw error;
    }
};
